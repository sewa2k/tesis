journal = {Mol. Immunol.},
keywords = {Animals,Aquatic Organisms,Aquatic Organisms: immunology,Biological Evolution,Fishes,Fishes: immunology,Fishes: virology,Humans,Immunity, Innate,Interferon Type I,Interferon Type I: immunology,Signal Transduction},
month = oct,

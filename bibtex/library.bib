keywords = {Animals,Electron,Flavobacterium,Flavobacterium: genetics,Flavobacterium: pathogenicity,Intestinal Mucosa,Intestinal Mucosa: microbiology,Intestinal Mucosa: ultrastructure,Microscopy,Oncorhynchus mykiss,Oncorhynchus mykiss: microbiology,Scanning,Scanning: veterinary,Species Specificity,Virulence},

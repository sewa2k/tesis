issn = {1050-4648},
journal = {Fish Shellfish Immunol.},
keywords = {Aeromonas hydrophila,Animals,Blood Chemical Analysis,Blood Chemical Analysis: veterinary,Cyprinidae,Dose-Response Relationship, Drug,Edwardsiella tarda,Enterobacteriaceae Infections,Enterobacteriaceae Infections: immunology,Enterobacteriaceae Infections: veterinary,Fish Diseases,Fish Diseases: immunology,Gram-Negative Bacterial Infections,Gram-Negative Bacterial Infections: immunology,Gram-Negative Bacterial Infections: veterinary,Immunity, Innate,Immunity, Innate: drug effects,Immunity, Innate: immunology,Injections, Intraperitoneal,Injections, Intraperitoneal: veterinary,Leukocytes,Leukocytes: drug effects,Linear Models,Muramidase,Muramidase: blood,Phagocytosis,Phagocytosis: drug effects,Superoxides,Superoxides: blood,beta-Glucans,beta-Glucans: administration \& dosage,beta-Glucans: pharmacology},

keywords = {Animals,Aquaculture,Bacteria,Bacteria: genetics,Bacteria: growth \& development,Bacteria: metabolism,Bass,Bass: growth \& development,Bass: microbiology,DNA Fingerprinting,DNA, Bacterial,DNA, Bacterial: genetics,Ecosystem,Feces,Feces: microbiology,Hydroxybutyrates,Hydroxybutyrates: metabolism,Intestines,Intestines: microbiology,Metagenome,Polyesters,Polyesters: metabolism},

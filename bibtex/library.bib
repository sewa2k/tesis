journal = {Fish Shellfish Immunol.},
keywords = {Adjuvants, Immunologic,Adjuvants, Immunologic: pharmacology,Animals,Cytokines,Cytokines: genetics,Fisheries,Fisheries: methods,Gene Expression Profiling,Gene Expression Regulation,Gene Expression Regulation: drug effects,Gene Expression Regulation: immunology,Immersion,Immunization,Immunization: methods,Immunization: veterinary,Lactoferrin,Lactoferrin: pharmacology,Oncorhynchus mykiss,Oncorhynchus mykiss: immunology,Plasmids,Plasmids: pharmacology,RNA, Messenger,RNA, Messenger: metabolism,Reverse Transcriptase Polymerase Chain Reaction,beta-Glucans,beta-Glucans: pharmacology},

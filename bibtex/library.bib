journal = {Gut Microbes},
keywords = {abbreviations,cfb,colitis,cytophaga-flavobacter-bacteroidetes,e,extracellular traps,h,haematoxylin and eosin,hd model,high dose streptomycin pre-treatment,inflammation,ld model,low dose streptomycin pre-treatment,microbiota,model,mpo,myeloperoxidase,nets,neutrophil,polymorphonuclear leukocyte,reactive oxygen species,ros,salmonella pathogenicity island,salmonella typhimurium,spi,t3ss,trflp,type iii secretion system},

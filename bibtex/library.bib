year = {2009}
}
@article{Aghaallaei2010,
abstract = {Chemokines and chemokine receptors are key evolutionary innovations of vertebrates. They are involved in morphogenetic processes and play an important role in the immune system. Based on an analysis of the chemokine receptor gene family in teleost genomes, and the expression patterns of chemokine receptor genes during embryogenesis and the wounding response in young larvae of Oryzias latipes, we identified the chemokine receptor cxcr3a as a marker of innate immune cells. Cells expressing cxcr3a were characterized in fish transgenic for a cxcr3a:gfp reporter. In embryos and larvae, cxcr3a-expressing cells are motile in healthy and damaged tissues, and phagocytic; the majority of these cells has the morphology of tissue macrophages, whereas a small fraction has a dendritic phenotype. In adults, cxcr3a-positive cells continue to specifically express myeloid-associate markers and genes related to antigen uptake and presentation. By light microscopy and ultrastructural analysis, the majority of cxcr3a-expressing cells has a dendritic phenotype, whereas the remainder resembles macrophage-like cells. After challenge of adult fish with bacteria or CpG oligonucleotides, phagocytosing cxcr3a-positive cells in the blood up-regulated il12p40 genes, compatible with their function as part of the mononuclear phagocytic system. Our results identify a marker of teleost mononuclear phagocytic cells and suggest a surprising degree of morphological and functional similarity between the innate immune systems of lower and higher vertebrates.},
author = {Aghaallaei, Narges and Bajoghli, Baubak and Schwarz, Heinz and Schorpp, Michael and Boehm, Thomas},
doi = {10.1073/pnas.1000467107},
file = {:home/sewa/Documentos/Papers/Proceedings of the National Academy of Sciences of the United States of America/Characterization of mononuclear phagocytic cells in medaka fish transgenic for a cxcr3agfp reporter. - Aghaallaei et al. - 2010.pdf:pdf},
issn = {1091-6490},
journal = {Proc. Natl. Acad. Sci. U. S. A.},
keywords = {Animals,Animals, Genetically Modified,Gene Expression Regulation, Developmental,Genes, Reporter,Green Fluorescent Proteins,Green Fluorescent Proteins: genetics,Oryzias,Oryzias: embryology,Oryzias: genetics,Oryzias: growth \& development,Phagocytes,Phagocytes: cytology,Receptors, CXCR3,Receptors, CXCR3: genetics},

journal = {Antimicrob. Agents Chemother.},
keywords = {Amino Acid Sequence,Animals,Antimicrobial Cationic Peptides,Antimicrobial Cationic Peptides: genetics,Antimicrobial Cationic Peptides: isolation \& purif,Antimicrobial Cationic Peptides: metabolism,Molecular Sequence Data,Oncorhynchus mykiss,Oncorhynchus mykiss: genetics,Oncorhynchus mykiss: metabolism,Phylogeny,Salmo salar,Salmo salar: genetics,Salmo salar: metabolism},

pmid = {22119937},
publisher = {Elsevier Ltd},
title = {{Quantitative and qualitative evaluation of iNOS expression in turbot (Psetta maxima) infected with Enteromyxum scophthalmi.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22119937},

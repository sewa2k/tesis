journal = {FEMS Immunol. Med. Microbiol.},
keywords = {Aeromonas salmonicida,Animals,Furunculosis,Furunculosis: immunology,Furunculosis: prevention \& control,Gram-Negative Bacterial Infections,Gram-Negative Bacterial Infections: immunology,Gram-Negative Bacterial Infections: prevention \& c,Lactobacillus,Lactobacillus: physiology,Lactococcus lactis,Lactococcus lactis: physiology,Leuconostoc,Leuconostoc: physiology,Oncorhynchus mykiss,Oncorhynchus mykiss: immunology,Oncorhynchus mykiss: microbiology,Probiotics,Probiotics: pharmacology},

journal = {Fish Shellfish Immunol.},
keywords = {Animals,Aquaculture,Dietary Supplements,Dietary Supplements: analysis,Fish Proteins,Fish Proteins: immunology,Fish Proteins: metabolism,Gadus morhua,Gadus morhua: immunology,Gadus morhua: metabolism,Gadus morhua: microbiology,Gene Expression Regulation,Immunity, Innate,Interferon-gamma,Interferon-gamma: metabolism,Interleukins,Interleukins: metabolism,Intestines,Intestines: immunology,Mannans,Mannans: administration \& dosage,Oligosaccharides,Oligosaccharides: administration \& dosage,Random Allocation,Rectum,Rectum: immunology,Saccharomyces cerevisiae,Vibrio,Vibrio: immunology,beta-Glucans,beta-Glucans: administration \& dosage},

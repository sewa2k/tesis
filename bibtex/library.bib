file = {:home/sewa/Documentos/Papers/Developmental and comparative immunology/Expression profiles of cytokines released in intestinal epithelial cells of the rainbow trout, Oncorhynchus mykiss, in response to bacte.pdf:pdf},
issn = {1879-0089},
journal = {Dev. Comp. Immunol.},
keywords = {Aeromonas salmonicida,Aeromonas salmonicida: immunology,Animals,Bacterial Adhesion,Cytokines,Cytokines: biosynthesis,Cytokines: immunology,Fish Diseases,Fish Diseases: immunology,Fish Diseases: microbiology,Furunculosis,Furunculosis: immunology,Furunculosis: veterinary,Gene Expression Profiling,Gram-Negative Bacterial Infections,Gram-Negative Bacterial Infections: immunology,Gram-Negative Bacterial Infections: veterinary,Immunity, Mucosal,Immunity, Mucosal: immunology,Intestinal Mucosa,Intestinal Mucosa: immunology,Intestinal Mucosa: microbiology,Oncorhynchus mykiss,Oncorhynchus mykiss: genetics,Oncorhynchus mykiss: immunology,Oncorhynchus mykiss: microbiology},

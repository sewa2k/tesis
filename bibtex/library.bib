file = {:home/sewa/Documentos/Papers/Fish \& shellfish immunology/Molecular characterization and expression analysis of six peroxiredoxin paralogous genes in gilthead sea bream (Sparus aurata) insight.pdf:pdf},
issn = {1095-9947},
journal = {Fish Shellfish Immunol.},
keywords = {Amino Acid Sequence,Animals,DNA, Complementary,Diet,Fish Diseases,Fish Diseases: immunology,Fish Diseases: parasitology,Fish Proteins,Fish Proteins: genetics,Fish Proteins: immunology,Fish Proteins: metabolism,Gene Expression Profiling,Intestinal Diseases, Parasitic,Intestinal Diseases, Parasitic: immunology,Intestinal Diseases, Parasitic: parasitology,Intestinal Diseases, Parasitic: veterinary,Molecular Sequence Data,Myxozoa,Peroxiredoxins,Peroxiredoxins: genetics,Peroxiredoxins: immunology,Peroxiredoxins: physiology,Polymerase Chain Reaction,Population Density,Sea Bream,Sea Bream: genetics,Sea Bream: immunology,Sea Bream: parasitology,Sea Bream: physiology,Stress, Physiological},

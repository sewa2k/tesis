title = {{Enteric salmonellosis disrupts the microbial ecology of the murine gastrointestinal tract.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2258829\&tool=pmcentrez\&rendertype=abstract},

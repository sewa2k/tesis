issn = {0145-305X},
journal = {Dev. Comp. Immunol.},
keywords = {Animals,Cell Line,Cell Movement,Cell Movement: drug effects,Chemokines,Chemokines: pharmacology,Flow Cytometry,Gene Expression Regulation,Gene Expression Regulation: drug effects,Interleukin-8,Interleukin-8: pharmacology,Macrophages,Macrophages: cytology,Macrophages: drug effects,Macrophages: immunology,Monocytes,Monocytes: cytology,Monocytes: drug effects,Monocytes: immunology,Oncorhynchus mykiss,Oncorhynchus mykiss: immunology,Oncorhynchus mykiss: metabolism,Phagocytes,Phagocytes: drug effects,Respiratory Burst,Respiratory Burst: drug effects},

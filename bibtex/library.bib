doi = {10.1186/1471-2172-12-69},
file = {:home/sewa/Documentos/Papers/BMC immunology/Impact of acute stress on antimicrobial polypeptides mRNA copy number in several tissues of marine sea bass (Dicentrarchus labrax). - Te.pdf:pdf},
issn = {1471-2172},
journal = {BMC Immunol.},
